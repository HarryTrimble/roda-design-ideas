{
  "name": "govuk-prototype-kit",
  "description": "Rapidly create HTML prototypes of GOV.UK services",
  "version": "13.1.0",
  "engines": {
    "node": "^16.x || >= 18.x"
  },
  "main": "index.js",
  "bin": {
    "govuk-prototype-kit": "bin/cli"
  },
  "files": [
    "bin/",
    "lib/",
    "migrator/",
    "prototype-starter/",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "LICENCE.txt",
    "README.md",
    "govuk-prototype-kit.config.json",
    "known-plugins.json",
    "index.js",
    "listen-on-port.js",
    "server.js",
    "start.js"
  ],
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "body-parser": "^1.14.1",
    "browser-sync": "^2.27.11",
    "chokidar": "^3.5.3",
    "cookie-parser": "^1.4.6",
    "cross-spawn": "^7.0.2",
    "csurf": "^1.11.0",
    "del": "^6.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "fs-extra": "^10.0.1",
    "inquirer": "^8.2.0",
    "lodash": "^4.17.21",
    "marked": "^4.0.10",
    "nodemon": "^2.0.20",
    "nunjucks": "^3.2.1",
    "portscanner": "^2.1.1",
    "require-dir": "^1.0.0",
    "sass": "^1.49.10",
    "session-file-store": "^1.5.0",
    "sync-request": "^6.0.0",
    "universal-analytics": "^0.5.3",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "govuk-frontend": ">=3"
  },
  "standard": {
    "ignore": [
      "__tests__/fixtures/",
      "migrator/known-old-versions/",
      "prototype-starter/"
    ],
    "plugins": [
      "cypress"
    ],
    "envs": [
      "cypress/globals"
    ]
  }
}
